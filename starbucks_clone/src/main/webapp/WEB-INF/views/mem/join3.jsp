		// 카드 등록
		function fn_cardReg(option) {
			window.option.customList = "";
			
			if (window.option.storeCodeTemp != undefined && window.option.storeCodeTemp != "") {
				window.option.customList = window.option.storeCodeTemp; 
			}
			
			if (window.option.customList != "") {
				window.option.customList += "|";
			}
			
			if (window.option.starCodeTemp != undefined && window.option.starCodeTemp != "") {
				window.option.customList += window.option.starCodeTemp; 
			}
			
			__ajaxCall("interface/myCardRegist", option , true, "JSON", "POST", 
					function(data) {
						var msg = "";		
						if( data.result_code == "SUCCESS"){
							starCodePopupClose(); //스타코드 입력 팝업창 닫기
							
							var jsonData = jQuery.parseJSON(data.data);
							var objParam = {
								cardRegNumber: jsonData.cardRegNumber,
								reqType: 'S'
							};
							
							
							___ajaxCall("interface/setMsrMyDelegateCardSet", objParam, false, "json", "post", function (_response) {
								if (_response.result_code == "SUCCESS") {
									
									var cardRegResultCode = parseInt(jsonData.cardRegResultCode);
									if(cardRegResultCode == 282 || cardRegResultCode == 283) { //약관 안내 문구일 경우 confirm창 호출 
										if(confirm(jsonData.cardRegMessage)){ //약관 페이지로 이동
											location.href = "footer/etc/rules_msr";
										}else { //보유카드 페이지로 이동
											location.href = "my/mycard_index?type=1";
										}
									} else { //카드 등록 성공시 alert창 노출 후 보유카드 페이지로 이동
										alert(jsonData.cardRegMessage);
										location.href = "my/mycard_index?type=1";
									}
									
								} else {
									var arr = _response.error_msg.split("|");
									if (arr.length == 2) {
										alert(arr[1]);
									} else {
										alert(arr[0]);
									}
									$("#CR2").focus();
								}
							});
							
						} else {
							msg = data.error_msg.split("|");
							
							if (parseInt(msg[0]) != 280) {
								starCodePopupClose(); //스타코드 입력 팝업창 닫기
							}
							
							if(msg.length > 1){
								var msg_code = parseInt(msg[0]);
								
								if(msg_code == 280) {
									alert(msg[1]);
									return;
								}else {
									alert(msg[1]);
								}
							}else{
								alert(msg[0]);
							}

							$("#CR2").focus();
							return;
						}
						
					},	
					function(message){
						alert(data.error_msg);
						return;
					}
			);

			$.commonLib.hideLoadingImg();
		}
		
