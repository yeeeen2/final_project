		//스타코드 등록 이벤트 처리 
		function fn_starCodeRegEvent() {
			$('#starCode').keyup(function () {
				if ($(this).val().length > $(this).attr('maxlength')) { 
					$(this).val($(this).val().substr(0, $(this).attr('maxlength'))); 
				}
				
				$(this).val($(this).val().replace(/[^a-zA-Z0-9]/g, "").toUpperCase());
			});
			
			$('#starCode').blur(function () {
				if ($(this).val().length > $(this).attr('maxlength')) { 
					$(this).val($(this).val().substr(0, $(this).attr('maxlength'))); 
				}
				
				$(this).val($(this).val().replace(/[^a-zA-Z0-9]/g, "").toUpperCase());
			});
			
			$("#starCodeReg").click(function(){ //스타코드 등록
				window.option.starCodeTemp = "";
			
				var starCode = $('#starCode');
				
				if ($.trim(starCode.val()) == '') {
					alert('Star Code 번호를 입력해 주세요. \nStar Code 이벤트 참여를 원하지 않는다면 SKIP 버튼을 눌러 주세요.');
					starCode.focus();
				}else if ( starCode.val().length != 10) {
					alert('올바른 Star Code 번호를 입력해 주세요.');
					starCode.focus();
				} else {
					var webTxt = "Star Code를 등록하시겠습니까?\n등록 완료된 Star Code는 등록 취소 및 재사용 불가합니다.";
					if(confirm(webTxt)) {
						$.commonLib.showLoadingImg();
						
						window.option.starCodeTemp = starCode.attr("data-eventCode") + ":" + starCode.val();
						fn_cardReg(window.option); //카드등록
					}
				}
				
			});
			
			$("#starCodeSkip").click(function(){ //스타코드 skip
				window.option.starCodeTemp = "";
			
				var webTxt = "Star Code 입력을 SKIP하시겠습니까?\n이 화면에서 Star Code를 입력하지 않을 경우,\nStar Code 이벤트 참여 불가합니다. \nSKIP 하더라도 스타벅스 카드는 정상 \n등록 처리됩니다.";
				if(confirm(webTxt)) {
					$.commonLib.showLoadingImg();
					
					fn_cardReg(window.option); //카드등록
				}
			});
		}
	</script>
