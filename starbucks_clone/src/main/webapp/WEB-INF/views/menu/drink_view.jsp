	function repalceAnd(str) {
		if(str != null) {
			//특수문자 앤드거나 한문 앤드인 경우
			str = str.replaceAll('&', '%26');
			str = str.replaceAll('＆', '%26');
		} else {
			str = "";
		}
		
		return str;
	}
