                    ]

                    var nutritionInfoView = '';
                    var unitValue = data.view.unit;

                    if(unitValue.indexOf('oz') === 0){
                    	var standardValue = parseFloat(data.view.standard);

                    	var mapping_info = nutrition_info.filter(function(item){
                    		return item.oz_standard === standardValue;
                   		});

						if(mapping_info.length > 0 && typeof mapping_info[0] !== 'undefined'){
							var info = mapping_info[0];
							//더블샷 & 나이트로 숏은 사이즈 이름 표기 안함
							if(info.oz_standard === 7 || info.oz_standard === 10){
