	// 수량형 빼기 처리
	function minusQty() {
		var $num    = $(this).next();
		var nQty    = Number($num.text());
		var nMinQty = Number($(this).data("minqty"));
		
		if (nQty > nMinQty) {
			$num.text(nQty - 1);
		}
		
	}
	
	// 수량형 더하기 처리
	function plusQty() {
		var $num         = $(this).prev();
		var nQty         = Number($num.text()); 
		var nMaxQty      = Number($(this).data("maxqty"));
		var strMaxQtyMsg = $(this).data("maxqtymsg");
		
		if (nQty >= nMaxQty) {
			if (strMaxQtyMsg != "") {
				alert(strMaxQtyMsg);	
			} else {
				alert("더 이상 추가 할 수 없습니다.");
			}
			
			return;
		}
		
		$num.text(nQty + 1);
	}
	
