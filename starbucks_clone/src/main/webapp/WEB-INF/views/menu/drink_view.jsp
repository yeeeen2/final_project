	function getCallingName() {
		var callingName = "";
		
		var cName_ice  = "";
		var cName_shot = "";
		var cName_shot_no_coffee = "";
		var cName_size = "";
		
		var arrCallingName = new Array();
		var nMinCallingSeq = 0;
		
		var qty = 1;
		
		// 1) 온도
		if (m_jsonMenuList[m_nMenuListIdx].delegateSku.hotIcedOnlyYn == "N"
				&& $("#delegateHotYn").val() == "N") {
			cName_ice = "ICE ";
		}
		
		// 2) 샷
		var $optShot = $('div.hidden_opt[data-optiontype="SHOT"]');
		$optShot.find(".p2 , .my_bev_select").each(function () {
			var stdRecipeFlag = $(this).data("stdrecipeflag");
			var applyType2    = $(this).data("applytype2");
			
			switch (applyType2) {
				case "01":
					var $select   = $(this).find("select");
					var customGrp = $select.data("customgrp");
					var curVal    = $select.val();
					var baseVal   = $select.data("defaultvalue");
					
					if (stdRecipeFlag == "Y" && curVal == baseVal) {
						// 기본옵션이면서 기본값이면 콜링네임에 표시 안 함.
					} else if (curVal == "") {
						// 선택 안함이면 콜링네임에 표시 안 함.
					} else {
						var data = $select.find("option:selected").data(); 
						arrCallingName[data.callingseq] = data.callingname + " ";
						if (nMinCallingSeq == 0
								|| nMinCallingSeq > Number(data.callingseq)) {
							nMinCallingSeq = Number(data.callingseq);
						}
					}		
					
					break;
				case "02":
					var $a        = $(this).find(".num");
					var customGrp = $a.data("customgrp");
					var curQty    = $a.text();
					
					var baseQty   = $a.data("baseqty");
					
					if (stdRecipeFlag == "Y" && curQty == baseQty) {
						// 기본옵션이면서 기본값이면 콜링네임에 표시 안 함.
					} else {
						if (customGrp == "S") {
							qty = curQty;
							if (curQty > 0) {
								
								switch(curQty) {
									case "0.5":	cName_shot = "하프 ";			break;
									case "1":   cName_shot = (bIsEspresso) ? "솔로 "         : "싱글 ";			break;
									case "1.5":	cName_shot = (bIsEspresso) ? "솔로앤하프 "   : "싱글앤하프 ";	break;
									case "2":   cName_shot = (bIsEspresso) ? "도피오 "       : "더블 ";			break;
									case "2.5":	cName_shot = (bIsEspresso) ? "도피오앤하프 " : "더블앤하프 ";	break;
									case "3":   cName_shot = "트리플 ";			break;
									case "3.5":	cName_shot = "트리플앤하프 ";	break;
									case "4":   cName_shot = "쿼드 ";			break;
									case "4.5":	cName_shot = "쿼드앤하프 ";		break;
									case "5":   cName_shot = "펜타 ";			break;
									case "5.5":	cName_shot = "펜타앤하프 ";		break;
									case "6":   cName_shot = "헥사 ";			break;
									case "6.5":	cName_shot = "헥사앤하프 ";		break;
									default:	cName_shot = curQty + " ";		break;

								}
								
							}
						} else {
							cName_shot_no_coffee = curQty + $a.data("callingname") + " ";
						}	
					}
					
					break;
				case "03":
					break;
			}
		});
		
		var $optShot = $('div.hidden_opt[data-optiontype="SHOT"]');
		$optShot.find(".p2 , .my_bev_select").each(function () {
			var stdRecipeFlag = $(this).data("stdrecipeflag");
			var applyType2    = $(this).data("applytype2");
			
			switch (applyType2) {
			case "01":
				if(qty==0){
					if(arrCallingName.length==8208){
						alert("디카페인 옵션은 에스프레소 샷에만 적용 가능합니다.");	
					}
					
					var $sel = $(this).find("select");
					var data = $sel.find("option:selected").data();
					var defaultValue = $sel.data("defaultvalue");
					$sel.val(defaultValue).trigger("change");
					arrCallingName[data.callingseq]="";
					showAllCurrentOption();
					
				}	
				break;
			}
		})
		
		// 3) 사이즈명
		var skuNo = m_jsonMenuList[m_nMenuListIdx].delegateSku.skuNo;
		var bIsEspresso = isEspresso(skuNo); 

		if (bIsEspresso == false) {
			switch($("#delegateSize").val()) {
				case "10": cName_size = "숏 ";		break;
				case "20": cName_size = "톨 ";		break;
				case "30": cName_size = "그란데 ";	break;
				case "40": cName_size = "벤티 ";	break;
				case "60": cName_size = "솔로 ";	break;
				case "70": cName_size = "도피오 ";	break;
				case "80": cName_size = "7oz ";		break;
			}
		}

		// *) 중간 정리 (온도 + 샷 + 사이즈 + 기타 등등..)
		callingName = cName_ice + cName_shot + cName_size + callingName;	
		
		
		// 4) 시럽
		// 5) 밀크
		// 6) CUSTOM
		$(".hidden_opt").each(function () {
			var optionType = $(this).data("optiontype");
			
			if (optionType != "SHOT") {
				$(this).find(".p2 , .my_bev_select").each(function () {
					var applyType2    = $(this).data("applytype2");
					var stdRecipeFlag = $(this).data("stdrecipeflag");
					
					switch (applyType2) {
						case "01":
							var $select   = $(this).find("select");
							var customGrp = $select.data("customgrp");
							var curVal    = $select.val();
							var baseVal   = $select.data("defaultvalue");
							
							if (stdRecipeFlag == "Y" && curVal == baseVal) {
								// 기본옵션이면서 기본값이면 콜링네임에 표시 안 함.
							} else if (curVal == "") {
								// 선택 안함이면 콜링네임에 표시 안 함.
							} else {
								var data = $select.find("option:selected").data(); 
								arrCallingName[data.callingseq] = data.callingname + " ";
								if (nMinCallingSeq == 0
										|| nMinCallingSeq > Number(data.callingseq)) {
									nMinCallingSeq = Number(data.callingseq);
								}
							}
							break;
						case "02":
							var $a        = $(this).find(".num");
							var customGrp = $a.data("customgrp");
							var curQty    = $a.text();
							var baseQty   = $a.data("baseqty");
							
							if (stdRecipeFlag == "Y" && curQty == baseQty) {
								// 기본옵션이면서 기본값이면 콜링네임에 표시 안 함.
							} else if (curQty == "0") {
								// 수량이 0이면 콜링네임에 표시 안 함.
							} else {
								var data = $a.data();
								arrCallingName[data.callingseq] = curQty + data.callingname + " ";
								if (nMinCallingSeq == 0
										|| nMinCallingSeq > Number(data.callingseq)) {
									nMinCallingSeq = Number(data.callingseq);
								}
							}
							break;
						case "03":
							break;
					}
				});
			}
		});
		
		for (var i = nMinCallingSeq; i < arrCallingName.length; i++) {
			if (arrCallingName[i] == undefined) {
				continue;
			}
			callingName += arrCallingName[i];
		}
		
		return callingName + cName_shot_no_coffee + m_jsonMenuList[m_nMenuListIdx].delegateSku.skuName;
	}
	
	function isEspresso(_skuNo) {
		var bIsEspresso = false;
		
		switch(_skuNo) {
			case "20":
			case "21":
			case "25":
			case "26":
			case "30":
			case "31":
				bIsEspresso = true;
				break;
		}
		
		return bIsEspresso;
	}
	
