    // 전화번호 (- 포함)
    ,getPhoneNumberWithHyphen : function (_phoneNumber) {
        var arrPhoneNumber = this.getPhoneNumberArray(_phoneNumber);

        return arrPhoneNumber[0] + "-" + arrPhoneNumber[1] + "-" + arrPhoneNumber[2];
